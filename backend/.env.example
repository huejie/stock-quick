# 环境变量示例文件

# 环境设置
ENV=development
DEBUG=true

# 数据库配置 (本地开发使用内存存储)
MONGODB_URL=mongodb://localhost:27017
DATABASE_NAME=stock_quick

# Redis 配置 (本地开发暂不使用)
REDIS_URL=redis://localhost:6379/0
REDIS_ENABLED=false

# 缓存配置
CACHE_EXPIRE_DEFAULT=30
CACHE_EXPIRE_SECTORS=60
CACHE_EXPIRE_NEWS=300

# 数据源配置
AKSHARE_TIMEOUT=10
